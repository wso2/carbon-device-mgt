{{#zone "navbars"}}
    <nav id="_uuf-navbar" class="navbar navbar-default" data-spy="affix" data-offset-top="50"
         data-offset-bottom="40">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                        data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                {{defineZone "navbarHeader"}}
            </div>
            <div id="navbar" class="navbar-collapse collapse" aria-expanded="false">
                <ul class="nav navbar-nav">
                    {{defineZone "navbarCollapsableLeftItems"}}
                </ul>
                {{defineZone "navbarCollapsableRightItems"}}
            </div>
        </div>
        {{defineZone "navbarBelow"}}
    </nav>
{{/zone}}